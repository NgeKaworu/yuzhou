{
  "name": "to-do-list",
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "spush": "git subtree push -P src/js-sdk js-sdk master",
    "spull": "git subtree pull -P src/js-sdk js-sdk master --squash"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.6.1",
    "@ant-design/icons": "^5.0.1",
    "@tanstack/react-query": "^4.26.0",
    "antd": "^5.2.3",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.7",
    "edk": "workspace:^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.2",
    "umi": "^4.0.55"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-color": "^3.0.6",
    "@types/react-dom": "^18.0.11",
    "@umijs/fabric": "^2.8.1",
    "@umijs/plugin-qiankun": "^2.42.0",
    "@umijs/plugins": "^4.0.55",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5"
  }
}
